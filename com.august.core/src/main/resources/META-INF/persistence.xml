<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="august">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.august.core.domain.BaseDomainObject</class>
		<class>com.august.core.domain.User</class>
		<class>com.august.core.domain.Authority</class>
		<class>com.august.core.domain.Expense</class>
		<class>com.august.core.domain.Group</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLInnoDBDialect" />
		</properties>
	</persistence-unit>

</persistence>
